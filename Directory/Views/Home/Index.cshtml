@model IEnumerable<global::Directory.Models.Lawsuit>
@{
    ViewBag.Title = "Home Page";
}

<h1>Шаблоны исков</h1>
<table class="table">
    @foreach (global::Directory.Models.Lawsuit item in Model)
    {
        <tr>
            <td>
                @Html.ActionLink(item.Name, "Form", new { id = item.Id })
            </td>
        </tr>
    }
</table>